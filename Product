package com.company;

public class Product {
    private String product_name;
    private double price;
    private int units;

    public Product(String product_name, double price, int units) {
        this.product_name = product_name;
        this.price = price;
        this.units = units;
    }

    public Product() {
    }

    public String toAString(){
        String output = "";
        output += this.product_name;
        return output;
    }

    public String getProduct_name() {
        return product_name;
    }

    public void setProduct_name(String product_name) {
        this.product_name = product_name;
    }

    public double getPrice() {
        return price;
    }

    public void setPrice(double price) {
        this.price = price;
    }

    public int getUnits() {
        return units;
    }

    public void setUnits(int units) {
        this.units = units;
    }
}
